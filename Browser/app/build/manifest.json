{
  "manifest_version": 3,
  "name": "Tabs",
  "description": "Extension custom",
  "version": "0.1",
  "icons": {
    "16": "images/extension-active/icon-16.png",
    "32": "images/extension-active/icon-32.png",
    "48": "images/extension-active/icon-48.png",
    "128": "images/extension-active/icon-128.png"
  },
  "action": { "default_popup": "views/popup/popup.html" },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "host_permissions": ["<all_urls>"],
  "permissions": ["scripting", "tabs", "webNavigation", "storage"]
}
